
pipeline {
 agent any

  stages {
     stage ('DEV ENV') {

     steps {
       withmaven(maven : 'maven') {
           sh 'mvn clean compile'
           }
        }
      }
      stage ('QA ENV') {

     steps {
       withmaven(maven : 'maven') {
           sh 'mvn clean package'
           }
        }
      }
     stage ('STG ENV') {

     steps {
       withmaven(maven : 'maven') {
           sh 'mvn clean test'
           }
        }
      }
    }
 }
